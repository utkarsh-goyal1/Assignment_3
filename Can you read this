import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        for(int i=0;i<s.length();i++)
        {
            if(i!=0){
                if(s.charAt(i)>='A' && s.charAt(i)<='Z'){
                System.out.println();
            }
            }
            
            System.out.print(s.charAt(i));
            
        }
    }
}
